{
  "name": "node-file-router",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build:esm": "cross-env BABEL_ENV=esm babel src --extensions '.ts' --out-dir 'dist/esm' --out-file-extension .mjs --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'dist/cjs' --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist/types --rootDir src",
    "build": "npm run build:cjs && npm run build:esm && npm run build:types && node ./dev/scripts/copy-files.js",
    "dev:link": "npm link ./dist",
    "dev:sandbox": "ts-node examples/sandbox/server.ts",
    "dev:sandbox:inspect": "node --loader ts-node --inspect examples/sandbox/server.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "module": "./esm/file-router.mjs",
  "main": "../cjs/file-router.js",
  "types": "./types/file-router.d.ts",
  "exports": {
    "import": "./esm/file-router.mjs",
    "require": "./cjs/file-router.js"
  },
  "keywords": [],
  "author": "Daniil Sitdikov",
  "license": "MIT",
  "engines": {
    "node": ">=14.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/plugin-transform-modules-commonjs": "7.21.5",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@types/node": "20.1.4",
    "@vitest/coverage-c8": "0.31.0",
    "babel-plugin-add-import-extension": "1.6.0",
    "cross-env": "7.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vitest": "0.31.0"
  }
}
